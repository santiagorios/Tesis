<div class="span12">
  <div class="well">
  <div class="page-header">
    <h1><%= @department.name %></h1>
  </div>
  </div>
</div>
<div class="span8">
  <div class="well">
  <% if !@programs.nil? and !@programs.empty? %>
    <%= render :partial => 'pages/section_object', :locals => { :collection => @programs }  %>
  <% end %>
  <% if !@projects.nil? and !@projects.empty? %>
    <%= render :partial => 'pages/section_object', :locals => { :collection => @projects }  %>
  <% end %>
  <% if !@entries.nil? and !@entries.empty? %>
    <%= render :partial => 'pages/section_object', :locals => { :collection => @entries }  %>
  <% end %>

  <% if !@knowledge_areas.nil? and !@knowledge_areas.empty? %>
    <%= render :partial => 'pages/section_object', :locals => { :collection => @knowledge_areas }  %>
  <% end %>
  </div>
</div>
<div class="span4">
  <%= render 'pages/sidebar' %>
</div>
