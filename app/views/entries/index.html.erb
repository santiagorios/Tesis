<%- model_class = Entry.new.class -%>
<div class="span8">
  <div class="page-header">
    <h1><%=t '.title', :default => model_class.model_name.human(:count => @entries.count )%></h1>
  </div>
  <ul class="thumbnails entries">
    <%= render @entries %>
  </ul>
  <div class="pull-right">
    <%= will_paginate @entries, :renderer => BootstrapPagination::Rails %>
  </div>
</div>
<div class="span4">
  <%= render "sidebar" %>
</div>