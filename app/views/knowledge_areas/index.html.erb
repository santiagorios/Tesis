<%- model_class = KnowledgeArea.new.class -%>
<div class="span8">
  <div class="well">
  <div class="page-header">
    <h1><%=t '.title', :default => model_class.model_name.human(:count => @knowledge_areas.count )%></h1>
  </div>
  <ul class="thumbnails entries">
    <%= render @knowledge_areas %>
  </ul>
  <div class="pull-right">
    <%= will_paginate @knowledge_areas, :renderer => BootstrapPagination::Rails %>
  </div>
  </div>
</div>
<div class="span4">
  <%= render "entries/sidebar" %>
</div>