  </div>
</div>
<div class="span8">
  <div class="well">
    <div class="page-header">
      <h2><%= t ("application." + model_class.model_name.downcase + ".empty_title") %></h2>
    </div>
    <div class="content">
      <%= t ("application." + model_class.model_name.downcase + ".empty_text") %>
    </div>
  </div>
</div>
<div class="span4">
  <%= render 'entries/sidebar' %>
</div>
