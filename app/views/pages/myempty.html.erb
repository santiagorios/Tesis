<%- model_class = model -%>
<div class="span12">
  <div class="well">
    <div class="page-header">
      <h1>Mis <%= model.table_name %></h1>
    </div>
  </div>
</div>
<div class="span8">
  <div class="well">
    <div class="page-header">
      <h2><%= t "application." + model_class.model_name.downcase + ".myempty_title_type", :type => type.name  %></h2>
    </div>
    <div class="content">
      <%= t "application." + model_class.model_name.downcase + ".myempty_text", :type => type.name, :object_name => model.name %>
    </div>
  </div>
</div>
<div class="span4">
  <%= render 'entries/sidebar' %>
</div>
