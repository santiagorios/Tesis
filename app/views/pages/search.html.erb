<div class="span12">
  <div class="well">
    <div class="page-header">
      <h1><%= t('application.search') %></h1>
    </div>
  </div>
</div>
<div class="span8">
  <div class="well">
    <% if !@programs.nil? and !@programs.empty? %>
      <%= render :partial => 'pages/section_search', :locals => { :collection => @programs }  %>
    <% end %>
    <% if !@projects.nil? and !@projects.empty? %>
      <%= render :partial => 'pages/section_search', :locals => { :collection => @projects }  %>
    <% end %>
    <% if !@results.nil? and !@results.empty? %>
      <%= render :partial => 'pages/section_search', :locals => { :collection => @results }  %>
    <% end %>
    <% if !@entries.nil? and !@entries.empty? %>
      <%= render :partial => 'pages/section_search', :locals => { :collection => @entries }  %>
    <% end %>


    <% if !@departments.nil? and !@departments.empty? %>
      <%= render :partial => 'pages/section_search', :locals => { :collection => @departments }  %>
    <% end %>
    <% if !@knowledge_areas.nil? and !@knowledge_areas.empty? %>
      <%= render :partial => 'pages/section_search', :locals => { :collection => @knowledge_areas }  %>
    <% end %>


    <% if !@research_lines.nil? and !@research_lines.empty? %>
      <%= render :partial => 'pages/section_search', :locals => { :collection => @research_lines }  %>
    <% end %>
    <% if !@research_sublines.nil? and !@research_sublines.empty? %>
      <%= render :partial => 'pages/section_search', :locals => { :collection => @research_sublines }  %>
    <% end %>

    <% if !@users.nil? and !@users.empty? %>
      <%= render :partial => 'pages/section_search', :locals => { :collection => @users }  %>
    <% end %>
  </div>
</div>
<div class="span4">
  <%= render "pages/sidebar" %>
</div>